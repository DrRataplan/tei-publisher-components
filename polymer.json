{
  "entrypoint": "index.html",
  "npm": true,
  "moduleResolution": "node",
  "sources": [
    "src/*.js",
    "package.json"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "bundle": true,
      "js": {
        "minify": true,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "addServiceWorker": true,
      "addPushManifest": true
    }
  ]
}